{
    "name": "ultreyaonline/standalone",
    "type": "project",
    "description": "The Ultreya Community Application",
    "keywords": [
        "application",
        "tresdias"
    ],
    "license": "MIT",
    "authors": [{"name": "Chris Brown"}],
    "require": {
        "php": "^7.3.3",
        "ext-redis": ">4.3",
        "bensampo/laravel-enum": "^1.33",
        "beyondcode/laravel-server-timing": "^1.1",
        "doctrine/dbal": "^2.10",
        "eluceo/ical": "^0.16.0",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.2",
        "illuminatech/config": "^1.1",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "lab404/laravel-impersonate": "^1.6",
        "laracasts/flash": "^3.1",
        "laravel/framework": "^7.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/telescope": "^3.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^1.0",
        "roave/security-advisories": "dev-master",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/laravel-activitylog": "^3.11",
        "spatie/laravel-backup": "^6.8",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-newsletter": "^4.8",
        "spatie/laravel-permission": "^3.11",
        "stripe/stripe-php": "^7.27"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-er-diagram-generator": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "sensiolabs/security-checker": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "check-security": [
            "vendor/bin/security-checker security:check"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan telescope:publish --ansi",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate"
        ]
    }
}
