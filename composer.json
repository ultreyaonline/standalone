{
    "name": "ultreyaonline/standalone",
    "type": "project",
    "description": "The Ultreya Community Application",
    "keywords": [
        "application",
        "tresdias"
    ],
    "license": "MIT",
    "authors": [{"name": "Chris Brown"}],
    "require": {
        "php": "^7.3|^8.0",
        "ext-redis": ">4.3",
        "bensampo/laravel-enum": "^3.0",
        "doctrine/dbal": "^3.0",
        "eluceo/ical": "^0.16.0",
        "facade/ignition": "^2.5",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminatech/config": "^1.3",
        "intervention/image": "^2.5",
        "lab404/laravel-impersonate": "^1.7.1",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.22.1",
        "laravel/slack-notification-channel": "^2.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^2.0",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-medialibrary": "^9.4",
        "spatie/laravel-newsletter": "^4.8",
        "spatie/laravel-permission": "^4.0",
        "stripe/stripe-php": "^7.50"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5.1",
        "barryvdh/laravel-ide-helper": "^2.8.1",
        "brianium/paratest": "^6.1",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "check-security": [
            "vendor/bin/security-checker security:check"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate"
        ]
    }
}
