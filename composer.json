{
    "name": "ultreyaonline/standalone",
    "type": "project",
    "description": "The Ultreya Community Application",
    "keywords": [
        "application",
        "tresdias"
    ],
    "license": "MIT",
    "authors": [{"name": "Chris Brown"}],
    "require": {
        "php": "^7.3.3",
        "ext-redis": ">4.3",
        "arcanedev/log-viewer": "^5.0",
        "bensampo/laravel-enum": "^1.26",
        "beyondcode/laravel-server-timing": "^1.1",
        "doctrine/dbal": "^2.9",
        "eluceo/ical": "^0.16.0",
        "facade/ignition": "^1.6",
        "fideloper/proxy": "^4.2",
        "genealabs/laravel-model-caching": "^0.7.0",
        "illuminatech/config": "^1.0",
        "intervention/image": "^2.5",
        "lab404/laravel-impersonate": "^1.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.12",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "roave/security-advisories": "dev-master",
        "spatie/flysystem-dropbox": "^1.1",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-backup": "^6.4",
        "spatie/laravel-medialibrary": "^7.6",
        "spatie/laravel-newsletter": "^4.6",
        "spatie/laravel-permission": "^3.0",
        "stripe/stripe-php": "^7.25",
        "yajra/laravel-datatables-buttons": "^4.8",
        "yajra/laravel-datatables-oracle": "^9.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-er-diagram-generator": "^1.4",
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "sensiolabs/security-checker": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "check-security": [
            "vendor/bin/security-checker security:check"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate"
        ]
    }
}
